<?xml version="1.0" encoding="utf-8"?>
<manifest package="csc296.finalproject"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label="@string/title_activity_sign_up">
        </activity>
        <activity
            android:name=".SignInActivity"
            android:label="@string/title_activity_sign_in">
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile">
        </activity>
        <activity
            android:name=".Fragtivity"
            android:label="@string/title_activity_fragtivity">
        </activity>
        <activity android:name=".RecyclerView.FeedRecyclerActivity">
        </activity>
        <activity android:name=".RecyclerView.PersonalRecyclerActivity">
        </activity>
        <activity android:name=".RecyclerView.PhotoDetailActivity">
        </activity>
        <activity android:name=".RecyclerView.CommentDetailActivity">
        </activity>
        <activity android:name=".Video.MediaPlayerActivity">
        </activity>
        <activity android:name=".masterdetail.MasterDetailActivity">
        </activity>
    </application>

</manifest>
